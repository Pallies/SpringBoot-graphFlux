type Customer{
    id: ID!
    name: String!
    age: Int
    orders: [CustomerOrder]!
    orders_:[CustomerOrder]!
    ordersLimit(limit:Int!) : [CustomerOrder]! @deprecated( reason : "use ordersBatch instead")
    ordersBatch: [CustomerOrder]!
    ordersBatch_better: [CustomerOrder]!
    address:Address
    account:Account @deprecated( reason : "use accountBatch instead")
    accountBatch:Account
}
input AgeRange{
    min: Int!
    max: Int!
}